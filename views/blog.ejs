class Solution {
    public int minOperations(int[] nums) {
        HashMap<Integer, Integer> mp = new HashMap<>();
        for(int i=0;i<nums.length;i++){
            mp.put(nums[i],0);
        }
        for (int i = 0; i < nums.length; i++) 
        { 
            if (mp.containsKey(nums[i]))  
            { 
                mp.put(nums[i], mp.get(nums[i]) + 1); 
            }  
            else
            { 
                mp.put(nums[i], 0); 
            } 
        } 
        int count=0;
        for (int i = 0; i <nums.length; i++)
        {
            if(mp.get(nums[i])%3==0){
            count+=((mp.get(nums[i]))/3);
        }
        else if(mp.get(nums[i])%2==0)
        {
            count+=((mp.get(nums[i]))/2);
            
            
        }

        else {
                return -1;
            }
        
    }
   return count;
}
}